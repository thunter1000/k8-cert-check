{
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.11.2",
    "babel-jest": "^27.0.6",
    "gts": "^3.1.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
